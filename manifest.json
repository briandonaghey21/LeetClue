{
    "manifest_version": 3,
    "name": "LeetHint",
    "version": "1.0",
    "description": "LeetCode extension to generate hints",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icons/LeetHintIcon.png"
      }
    },
    "permissions": ["activeTab", "tabs", "scripting"],
    "content_scripts": [
      {
        "matches": ["*://leetcode.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "icons/LeetHintIcon.png"
    }
  }
  