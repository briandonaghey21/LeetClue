{
    "manifest_version": 3,
    "name": "LeetHint",
    "version": "1.0",
    "description": "LeetCode extension to generate hints",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icons/LeetClueIcon.png"
      }
    },
    "permissions": ["activeTab", "tabs", "scripting"],
    "content_scripts": [
      {
        "matches": ["*://leetcode.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
}
  }
  